#: src/main.c:35
#, c-format
msgid "%s reads a paradox file and outputs the file in CSV format."
msgstr "%s liest eine Paradox Datei und gibt den Inhalt im CSV-Format aus."

#: src/main.c:37
#, c-format
msgid "%s reads a paradox file and outputs the file in SQL format."
msgstr "%s liest eine Paradox Datei und gibt den Inhalt im SQL-Format aus."

#: src/main.c:39
#, c-format
msgid "%s reads a paradox file and outputs the file in HTML format."
msgstr "%s liest eine Paradox Datei und gibt den Inhalt im HTML-Format aus."

#: src/main.c:41
#, c-format
msgid ""
"%s reads a paradox file and outputs information about the file\n"
"or dumps the content in CSV, HTML or SQL format."
msgstr ""
"%s liest eine Paradox Datei und zeigt entweder Informationen über die\n"
"Datei an oder gibt den Inhalt im CSV-, HTML- oder SQL-Format aus."

#: src/main.c:44
#, c-format
msgid "Usage: %s [OPTIONS] FILE"
msgstr "Aufruf: %s [OPTION] DATEI"

#: src/main.c:46
msgid "Options:"
msgstr "Optionen:"

#: src/main.c:48
msgid "  -h, --help          this usage information."
msgstr "  -h, --help          diese Hilfe."

#: src/main.c:50
msgid "  -v, --verbose       be more verbose."
msgstr "  -v, --verbose       mehr Informationen anzeigen"

#: src/main.c:53
msgid "  -i, --info          show information about file."
msgstr "  -i, --info          zeige Informationen über eine Datei."

#: src/main.c:55
msgid "  -c, --csv           dump records in CSV format."
msgstr "  -c, --csv           gebe Datensätze im CSV-Format aus."

#: src/main.c:57
msgid "  -s, --sql           dump records in SQL format."
msgstr "  -s, --sql           gebe Datensätze im SQL-Format aus."

#: src/main.c:59
msgid "  -x, --html          dump records in HTML format."
msgstr "  -x, --html          gebe Datensätze im HTML-Format aus."

#: src/main.c:61
msgid "  -t, --shema         output schema of database."
msgstr "  -t, --shema         gebe Schema der Datenbank aus."

#: src/main.c:63
msgid "  --mode=MODE         set output mode (csv, sql, or schema)."
msgstr "  --mode=MODE         setze Ausgabemodus (csv, sql oder schema)."

#: src/main.c:66
msgid "  -o, --output=FILE   output data into file instead of stdout."
msgstr "  -o, --output=DATEI  schreibe Ausgabe in Datei statt nach stdout."

#: src/main.c:68
msgid "  -b, --blobfile=FILE read blob data from file."
msgstr "  -b, --blobfile=DATEI lese Blobs aus Datei."

#: src/main.c:70
msgid "  -p, --blobprefix=PREFIX prefix for all created files with blob data."
msgstr "  -p, --blobprefix=PRÄFIX Präfix für erzeugte Blob-Dateien."

#: src/main.c:72
msgid "  -n, --primary-index-file=FILE read primary index from file."
msgstr "  -n, --primary-index-file=DATEI lese Primärindex aus Datei."

#: src/main.c:74
msgid "  -r, --recode=ENCODING sets the target encoding."
msgstr "  -r, --recode=ENCODING setzt Zielkodierung"

#: src/main.c:77
msgid "  --separator=CHAR    character used to separate field values."
msgstr ""
"  --separator=CHAR    Zeichen, das zur Trennung von Feldern benutzt wird."

#: src/main.c:79
msgid "  --enclosure=CHAR    character used to enclose field values."
msgstr ""
"  --enclosure=CHAR    Zeichen, das zum Einschließen von Feldern benutzt wird."

#: src/main.c:83
msgid "  --mark-deleted      add extra column with 1 for deleted records."
msgstr ""
"  --mark-deleted      Ausgabe einer zusätzlichen Spalte mit 1 für gelöschte "
"DS"

#: src/main.c:86
msgid "  --include-blobs     add blob fields in sql output."
msgstr "  --include-blobs     berücksichtige Blobs beim der SQL-Ausgabe."

#: src/main.c:88
msgid "  --fields=REGEX      extended regular expression to select fields."
msgstr ""
"  --fields=REGEX      erweiterter regulärer Ausdruck zur Auswahl von Feldern."

#: src/main.c:90
msgid "  --output-deleted    output also records which were deleted."
msgstr "  --output-deleted    gebe auch gelöschte Datensätze aus."

#: src/main.c:93
msgid "  --tablename=NAME    overwrite name of database table."
msgstr "  --tablename=NAME    überschreibe den Namen der Datenbanktable."

#: src/main.c:97
msgid "  --delete-table      delete existing sql database table."
msgstr "  --delete-table      lösche bestehende SQL Datenbanktable."

#: src/main.c:102
msgid ""
"If you do not specify any of the options -i, -c, -s, -x, or -t\n"
"then -i will be used."
msgstr ""
"Wenn Sie keine der Optionen -i, -c, -s, -x oder -t angeben,\n"
"dann wird die Option -i verwendet."

#: src/main.c:106
msgid "The option --fields will only affect csv, html and sql output."
msgstr "Die Option --fields beeinflusst nur die CSV-, HTML- und SQL-Ausgabe."

#: src/main.c:108
msgid ""
"The options --separator, --enclosure and --mark-deleted will only affect csv "
"output."
msgstr ""
"Die Optionen --separator, --enclosure und --mark-deleted beeinflussen nur "
"die CSV-Ausgabe."

#: src/main.c:112
msgid ""
"If exporting csv format fields will be separated by tabulator\n"
"and enclosed into \"."
msgstr ""
"Beim Export im CSV-Format werden Felder standardmäßig mit einem\n"
"Tabulator getrennt und durch Anführungsstriche eingeschlossen."

#: src/main.c:119
msgid "libpx uses librecode for recoding."
msgstr "libpx benutzt librecode zum reencodieren."

#: src/main.c:122
msgid "libpx uses iconv for recoding."
msgstr "libpx benutzt iconv zum reencodieren."

#: src/main.c:125
msgid "libpx has no support for recoding."
msgstr "libpx unterstützt nicht das reencodieren."

#: src/main.c:130
msgid "libpx has been compiled for big endian architecture."
msgstr "libpx wurde für Architekturen mit big endian übersetzt."

#: src/main.c:132
msgid "libpx has been compiled for little endian architecture."
msgstr "libpx wurde für Architekturen mit little endian übersetzt."

